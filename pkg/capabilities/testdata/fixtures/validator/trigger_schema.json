{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/smartcontractkit/chainlink/capabilities/root",
  "properties": {
    "config": {
      "properties": {
        "foo": {
          "$schema": "https://json-schema.org/draft/2020-12/schema",
          "items": true,
          "type": "array"
        },
        "bar": {
          "type": "integer"
        },
        "bonk": {
          "$schema": "https://json-schema.org/draft/2020-12/schema",
          "type": "object"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "foo",
        "bar",
        "bonk"
      ]
    },
    "outputs": {
      "properties": {
        "quux": {
          "type": "string"
        },
        "corge": {
          "type": "integer",
          "minimum": 1
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "quux",
        "corge"
      ]
    }
  },
  "additionalProperties": false,
  "type": "object",
  "required": [
    "config",
    "outputs"
  ]
}